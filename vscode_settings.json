{
    "terminal.integrated.shell.windows": "C:\\windows\\System32\\cmd.exe",
    "git.autofetch": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "python.globalModuleInstallation": true,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "python.dataScience.alwaysTrustNotebooks": true,
    "launch": {
        "configurations": [
        {
            "name": "Python : Debug CGDM",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            // "console": "externalTerminal",
            "env": {"LOG_LEVEL":"DEBUG",
                    "SERVER_ADDRESS":"0.0.0.0",
                    "SERVER_PORT":"8080",
                    "HOSTNAME":"local",
                    "VERSION_SERVER":"0.1",
                    "CI_COVERAGE_ENABLED":"false"},
            "cwd": "${workspaceFolder}/app"
        }
    ],
        "compounds": []
    },
    "workbench.statusBar.visible": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations": {
        "terminal.background":"#090300",
        "terminal.foreground":"#d4d4d4",
        "terminalCursor.background":"#A5A2A2",
        "terminalCursor.foreground":"#A5A2A2",
        "terminal.ansiBlack":"#090300",
        "terminal.ansiBlue":"#01A0E4",
        "terminal.ansiBrightBlack":"#5C5855",
        "terminal.ansiBrightBlue":"#01A0E4",
        "terminal.ansiBrightCyan":"#B5E4F4",
        "terminal.ansiBrightGreen":"#01A252",
        "terminal.ansiBrightMagenta":"#A16A94",
        "terminal.ansiBrightRed":"#DB2D20",
        "terminal.ansiBrightWhite":"#F7F7F7",
        "terminal.ansiBrightYellow":"#FDED02",
        "terminal.ansiCyan":"#B5E4F4",
        "terminal.ansiGreen":"#01A252",
        "terminal.ansiMagenta":"#A16A94",
        "terminal.ansiRed":"#DB2D20",
        "terminal.ansiWhite":"#A5A2A2",
        "terminal.ansiYellow":"#FDED02"
    },
    "git.confirmSync": false,
    "window.zoomLevel": 0,
    // -TODO-TREE CONFIG
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.tags": [
        "TOTEST",
        "TODO",
        "HACK",
        "FIXME",
        "BUG",
        "DONE",
        "LATER"
    ],
    "todo-tree.tree.hideIconsWhenGroupedByTag": true,
    "todo-tree.highlights.defaultHighlight": {
        "background": "black",
        // "fontWeight":"bold"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "foreground": "yellow",
            "icon": "check",
            "iconColour": "yellow",
            "type": "line"
        },
        "TOTEST": {
            "foreground": "cyan",
            "icon": "checklist",
            "iconColour": "cyan",
            "type": "line"
        },
        "DONE": {
            "foreground": "green",
            "icon": "thumbsup",
            "iconColour": "green",
            "type": "line"
        },
        "FIXME": {
            "foreground": "red",
            "icon": "alert",
            "iconColour": "red",
            "type": "line"
        },
        "LATER": {
            "foreground": "grey",
            "icon": "history",
            "iconColour": "grey",
            "type": "line"
        }
    },
    "todo-tree.general.tagGroups": {
        "FIXME": [
            "FIXME",
            "BUG"
        ]
    }
}